{
  "name": "linkskool",
  "version": "1.1.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --publish always",
    "cbt-dashboard-watch": "browserify renderer/cbt-dashboard.js -o js-bundle/cbt-dashboard.js --watch",
    "index-watch": "browserify renderer/index.js -o js-bundle/index.js --watch",
    "key-buddy-watch": "browserify renderer/key-buddy.js -o js-bundle/key-buddy.js --watch",
    "learning-watch": "browserify renderer/learning.js -o js-bundle/learning.js --watch",
    "cbt-watch": "browserify renderer/cbt.js -o js-bundle/cbt.js --watch",
    "summary-watch": "browserify renderer/summary.js -o js-bundle/summary.js --watch",
    "learn-course-watch": "browserify renderer/learn-course.js -o js-bundle/learn-course.js --watch",
    "quiz-watch": "browserify renderer/quiz.js -o js-bundle/quiz.js --watch",
    "video-watch": "browserify renderer/video.js -o js-bundle/video.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "asar": true,
    "appId": "com.digitaldreams.linkskool",
    "win": {
      "icon": "assets/icons/logo2.ico",
      "target": "nsis"
    },
    "nsis": {
      "runAfterFinish": true,
      "perMachine": true,
      "allowElevation": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://linkschoolonline.com/assets/downloads/"
      }
    ]
  },
  "keywords": [],
  "author": "ToochiDennis",
  "license": "ISC",
  "description": "LinkSkool",
  "devDependencies": {
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "better-sqlite3": "^11.8.1",
    "browserify": "^17.0.1",
    "chart.js": "^4.4.8",
    "electron-log": "^5.3.3",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^6.3.9",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12"
  }
}