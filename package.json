{
  "name": "LinkSkool",
  "version": "1.0.8",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder --win --publish always",
    "watch": "browserify renderer/index.js -o renderer/index-bundle.js --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "build": {
    "asar": true,
    "appId": "com.digitaldreams.linkskool",
    "win": {
      "icon": "assets/icons/logo2.ico",
      "target": "nsis"
    },
    "nsis": {
      "runAfterFinish": false,
      "perMachine": true,
      "allowElevation": true
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://linkschoolonline.com/assets/downloads/"
      }
    ]
  },
  "keywords": [],
  "author": "ToochiDennis",
  "license": "ISC",
  "description": "LinkSkool",
  "devDependencies": {
    "electron": "^34.0.2",
    "electron-builder": "^25.1.8",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-reloader": "^1.2.3"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "better-sqlite3": "^11.8.1",
    "browserify": "^17.0.1",
    "chart.js": "^4.4.8",
    "electron-log": "^5.3.3",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^6.3.9",
    "node-fetch": "^3.3.2",
    "node-machine-id": "^1.1.12"
  }
}